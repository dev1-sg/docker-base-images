{
  "image": "public.ecr.aws/dev1-sg/devcontainer/debian-base:latest",
  "name": "dev1-sg-docker-base-images",
  "features": {
    "ghcr.io/dev1-sg/devcontainers/docker-in-docker:latest": {}
  },
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "zsh"
      },
      "extensions": [
        "github.codespaces",
        "ms-vscode-remote.remote-containers"
      ]
    }
  },
  "containerUser": "vscode",
  "workspaceMount": "source=${localWorkspaceFolder}/,target=/home/vscode/dev1-sg-docker-base-images,type=bind,consistency=cached",
  "workspaceFolder": "/home/vscode",
  "postCreateCommand": "sudo chown -R vscode:vscode /home/vscode"
}
